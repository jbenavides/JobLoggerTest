<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="LevelLoggerConfigurationSection" type="JobLoggerApp.Helpers.LevelLoggerConfigurationSection, JobLoggerApp" />
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <connectionStrings>
    <add name="JobLoggerConnectionString" connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\App_Data\JobLoggerDB.mdf;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <LevelLoggerConfigurationSection
    isMessageTypeAllowed="true"
    isWarningTypeAllowed="true"
    isErrorTypeAllowed="true" />
</configuration>